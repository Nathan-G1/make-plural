    "browserify": "^16.2.3",
    "cldr-core": "^34.0.0",
    "expect.js": "*",
    "http-server": "^0.11.1",
    "mocha": "^6.1.4",
    "standard": "^12.0.1",
    "uglify-js": "^3.5.12"
  },
  "optionalDependencies": {
    "minimist": "^1.2.0"
  }
